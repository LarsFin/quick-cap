{
  "name": "@quick-cap/storage",
  "version": "1.0.0",
  "description": "Service responsible for data management, a thin API layer sits above the integrated database, using an ORM to allow for dynamic database integration.",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "ts-node-dev --respawn --transpile-only src/main/index.ts",
    "dev:migrate": "prisma migrate dev",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "node build/index.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "node scripts/run-integration-tests.js",
    "test:unit": "jest --config jest.config.unit.js"
  },
  "author": "LarsFin",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.27.0",
    "@jest/globals": "^29.7.0",
    "@tsconfig/recommended": "^1.0.8",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jest-when": "^3.5.5",
    "@types/pg": "^8.11.11",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^3.8.7",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "jest-when": "^3.7.0",
    "pg": "^8.14.1",
    "prettier": "^3.5.3",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.2",
    "uuid": "^11.1.0"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "body-parser": "^1.20.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "prisma": "^6.5.0",
    "zod": "^3.24.2"
  }
}
